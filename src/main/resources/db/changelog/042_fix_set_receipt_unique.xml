<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">

    <changeSet id="042_fix_set_receipt_unique" author="mrojek">

        <dropUniqueConstraint tableName="set_receipt"
                              constraintName="uq_set_return_number"/>

        <addUniqueConstraint tableName="set_receipt"
                             columnNames="receipt_number"
                             constraintName="uq_set_receipt_number"/>
    </changeSet>


</databaseChangeLog>